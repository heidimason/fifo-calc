{
  "expo": {
    "name": "Profit Calculator",
    "description": "React Native app which calculates profit based on purchase/sale shares and FIFO accounting method",
    "slug": "fifo-calc",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.heidimason.fifocalculator"
    },
    "android": {
			"package": "com.heidimason.fifocalculator"
		}
  }
}
